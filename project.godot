; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="AMC Forth"
run/main_scene="res://terminal_server_3d.tscn"
config/features=PackedStringArray("4.3", "C#", "Forward Plus")
config/icon="res://icon.svg"

[display]

window/size/viewport_width=900
window/size/viewport_height=540

[dotnet]

project/assembly_name="AMC Forth"

[editor_plugins]

enabled=PackedStringArray("res://addons/amc_forth/plugin.cfg", "res://addons/amc_forth_terminal/plugin.cfg", "res://addons/gdLinter/plugin.cfg", "res://addons/gdscript2all/plugin.cfg", "res://addons/gdscript_formatter/plugin.cfg", "res://addons/script-tabs/plugin.cfg")

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
